CREATE TABLE [dbo].[Usuarios] (
    [Id]       INT           NOT NULL,
    [nombre]   NVARCHAR (50) NULL,
    [apellido] NVARCHAR (50) NULL,
    [email]    NVARCHAR (50) NULL,
    [password] NVARCHAR (50) NULL
);
